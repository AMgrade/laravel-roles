{
    "name": "amgrade/laravel-roles",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Borzyonok",
            "email": "dima.matters@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/database": "^10.0",
        "illuminate/http": "^10.0",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "orchestra/testbench": "^8.0",
        "orchestra/testbench-core": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "AMgrade\\LaravelRoles\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AMgrade\\LaravelRoles\\Tests\\": "tests/",
            "AMgrade\\LaravelRoles\\Tests\\Database\\Factories\\": "tests/database/factories/",
            "AMgrade\\LaravelRoles\\Tests\\Database\\Seeders\\": "tests/database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AMgrade\\LaravelRoles\\LaravelRolesServiceProvider"
            ]
        }
    }
}
