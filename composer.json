{
    "name": "mcmatters/laravel-roles",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Borzyonok",
            "email": "dima.matters@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/database": "^8.0 || ^9.0",
        "illuminate/http": "^8.0 || ^9.0",
        "illuminate/support": "^8.0 || ^9.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.0",
        "orchestra/testbench": "^6.0",
        "orchestra/testbench-core": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "McMatters\\LaravelRoles\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "McMatters\\LaravelRoles\\Tests\\": "tests/",
            "McMatters\\LaravelRoles\\Tests\\Database\\Factories\\": "tests/database/factories/",
            "McMatters\\LaravelRoles\\Tests\\Database\\Seeders\\": "tests/database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "McMatters\\LaravelRoles\\ServiceProvider"
            ]
        }
    }
}
