{
    "name": "mcmatters/laravel-roles",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Borzyonok",
            "email": "dima.matters@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/database": "^5.6",
        "illuminate/http": "^5.6",
        "illuminate/support": "^5.6"
    },
    "require-dev": {
        "orchestra/testbench": "3.7.6",
        "orchestra/testbench-core": "3.7.7"
    },
    "autoload": {
        "psr-4": {
            "McMatters\\LaravelRoles\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "McMatters\\LaravelRoles\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/database/seeds"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "McMatters\\LaravelRoles\\ServiceProvider"
            ]
        }
    }
}
