{
    "name": "mcmatters/laravel-roles",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Borzyonok",
            "email": "dima.matters@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/database": "^5.8 || 6.* || 7.*",
        "illuminate/http": "^5.8 || 6.* || 7.*",
        "illuminate/support": "^5.8 || 6.* || 7.*"
    },
    "require-dev": {
        "fzaninotto/faker": "1.8.0",
        "orchestra/testbench": "3.8.5",
        "orchestra/testbench-core": "3.8.7",
        "phpunit/phpunit": "7.5.4"
    },
    "autoload": {
        "psr-4": {
            "McMatters\\LaravelRoles\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "McMatters\\LaravelRoles\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/database/seeds"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "McMatters\\LaravelRoles\\ServiceProvider"
            ]
        }
    }
}
